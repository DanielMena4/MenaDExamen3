<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MenaDExamen3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MenaDExamen3"
    Shell.FlyoutBehavior="Disabled"
    Title="MenaDExamen3">

    <TabBar>
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        <Tab Title="Listado">
            <ShellContent ContentTemplate="{DataTemplate local:ListadoPage}" />
        </Tab>
        <Tab Title="Registro">
            <ShellContent ContentTemplate="{DataTemplate local:RegistroPage}" />
        </Tab>
    </TabBar>


</Shell>
